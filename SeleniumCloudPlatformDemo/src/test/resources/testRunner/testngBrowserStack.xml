<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BrowserStack Lab regression suite" thread-count="3" parallel="true" verbose="3">
  
  <test name="BrowserStack Test - Chrome">
  	<parameter name="browser" value="Chrome"/>
  	<parameter name="version" value="85.0"/>
  	<parameter name="os" value="OS X"/>
  	<parameter name="os_version" value="Catalina"/>
    <classes>
      <class name="com.qa.test.SauceTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="BrowserStack Test - ff">
  	<parameter name="browser" value="Firefox"/>
  	<parameter name="version" value="80.0"/>
  	<parameter name="os" value="Windows"/>
  	<parameter name="os_version" value="10"/>
    <classes>
      <class name="com.qa.test.SauceTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
